{
    "env": {
        "browser": true,
        "builtin": true,
        "jasmine": true,
        "mocha": true,
        "node": true
    },
    "globals": {
        "$": true,
        "Backbone": true,
        "Modernizr": true,
        "Promise": true,
        "_": true,
        "define": true,
        "i18n": true,
        "require": true,
        "requirejs": true,
        "window": true
    },
    "rules": {
        "block-scoped-var": 2,
        "camelcase": 1,
        "callback-return": 1,
        "comma-style": [
            2,
            "last"
        ],
        "curly": [
            2,
            "all"
        ],
        "dot-notation": [
            2,
            {
                "allowKeywords": true
            }
        ],
        "eqeqeq": [
            2,
            "allow-null"
        ],
        "guard-for-in": 2,
        "new-cap": 2,
        "no-bitwise": 2,
        "no-caller": 2,
        "no-cond-assign": [
            2,
            "except-parens"
        ],
        "no-constant-condition": 2,
        "no-debugger": 2,
        "no-empty": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": [
            2,
            "functions"
        ],
        "no-extra-semi": 2,
        "no-inner-declarations": 2,
        "no-irregular-whitespace": 2,
        "no-iterator": 2,
        "no-loop-func": 2,
        "no-multi-str": 2,
        "no-new": 0,
        "no-new-require": 1,
        "no-nested-ternary": 2,
        "no-plusplus": 2,
        "no-proto": 2,
        "no-script-url": 2,
        "no-sequences": 2,
        "no-shadow": 1,
        "no-undef": 2,
        "no-unreachable": 2,
        "no-undefined": 2,
        "no-unused-vars": 2,
        "no-unneeded-ternary": 2,
        "no-use-before-define": 1,
        "no-with": 2,
        "one-var": 1,
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            0,
            "never"
        ],
        "strict": [
            2,
            "function"
        ],
        "valid-typeof": 2,
        "vars-on-top": 1,
        "wrap-iife": [
            2,
            "inside"
        ]
    }
}
